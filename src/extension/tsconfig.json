{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "express"],
    "outDir": "../../extension",
    "baseUrl": ".",
    "paths": {
      "*": ["../@types/*"]
    }
  }
}
